<module>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.common.GoogleCommonBase' />
	<inherits name='com.googlecode.gwtx.Java' />
	<inherits name='com.google.gwt.activity.Activity' />
	<inherits name='com.totsp.gwittir.GwittirNoDeps' />
	<inherits name='com.totsp.gwittir.LoggingWarn' />
	
	<inherits name='cc.alcina.framework.emul.AlcinaEmul' />
	<inherits name='cc.alcina.framework.emul.AlcinaEmulDevModeRegex' />
	<replace-with class="cc.alcina.framework.common.client.util.StackFrameHelperIE">
		<when-type-is class="cc.alcina.framework.common.client.util.StackFrameHelper" />
		<when-property-is name="user.agent" value="ie6" />
	</replace-with>
	<replace-with class="cc.alcina.framework.common.client.util.StackFrameHelperIE">
		<when-type-is class="cc.alcina.framework.common.client.util.StackFrameHelper" />
		<when-property-is name="user.agent" value="ie8" />
	</replace-with>
	<define-configuration-property name="alcina.introspectorFilter.classname" is-multi-valued="false"/>
	<define-configuration-property name="alcina.SimpleCssResourceGenerator.ignoreMissingDataUrls" is-multi-valued="false"/>
	<set-configuration-property name="alcina.SimpleCssResourceGenerator.ignoreMissingDataUrls" value="false"/>
	<define-configuration-property name="alcina.introspectorFilter.dataFile" is-multi-valued="false"/>
	<source path="client" />
</module>