<module>
	<inherits name="com.google.gwt.user.User" />
	<inherits name="rocket.util.Util" />

	<replace-with class="rocket.selection.client.support.FireFoxSelectionSupport">
		<when-type-is class="rocket.selection.client.support.SelectionSupport" />
		<when-property-is name="user.agent" value="gecko" />
	</replace-with>

	<replace-with class="rocket.selection.client.support.FireFoxSelectionSupport">
		<when-type-is class="rocket.selection.client.support.SelectionSupport" />
		<when-property-is name="user.agent" value="gecko1_8" />
	</replace-with>

	<replace-with class="rocket.selection.client.support.InternetExplorerSelectionSupport">
		<when-type-is class="rocket.selection.client.support.SelectionSupport" />
		<when-property-is name="user.agent" value="ie6" />
	</replace-with>
	<replace-with class="rocket.selection.client.support.InternetExplorerSelectionSupport">
		<when-type-is class="rocket.selection.client.support.SelectionSupport" />
		<when-property-is name="user.agent" value="ie8" />
	</replace-with>
	<replace-with class="rocket.selection.client.support.InternetExplorerSelectionSupport">
		<when-type-is class="rocket.selection.client.support.SelectionSupport" />
		<when-property-is name="user.agent" value="ie9" />
	</replace-with>
	<replace-with class="rocket.selection.client.support.OperaSelectionSupport">
		<when-type-is class="rocket.selection.client.support.SelectionSupport" />
		<when-property-is name="user.agent" value="opera" />
	</replace-with>

	<replace-with class="rocket.selection.client.support.SafariSelectionSupport">
		<when-type-is class="rocket.selection.client.support.SelectionSupport" />
		<when-property-is name="user.agent" value="safari" />
	</replace-with>
</module>